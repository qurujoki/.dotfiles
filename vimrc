hi LineNrAbove ctermfg=DarkGrey
hi LineNrBelow ctermfg=DarkGrey

set autoindent
set background=""
set backspace=indent,eol,start
set expandtab
set formatoptions="jroql"
set hlsearch
set ignorecase
set incsearch
set laststatus=2
set linebreak
set noswapfile
set number relativenumber
set path+=**
set shiftwidth=0
set smartcase
set smartindent
set smarttab
set softtabstop=-1
set tabstop=4
set textwidth=80
set wildmenu
set wildoptions=pum

syntax on
